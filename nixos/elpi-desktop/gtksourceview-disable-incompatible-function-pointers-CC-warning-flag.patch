From 6106af177b60ebf2585f9823bdbc391f3563cf7d Mon Sep 17 00:00:00 2001
From: TheDSCPL <kpvrzlzzx@mozmail.com>
Date: Tue, 28 May 2024 13:22:52 +0000
Subject: [PATCH] disable incompatible function pointers CC warning flag

---
 meson.buildd | +++---
 1 file changed, insertions(+), 3 deletions(-)

diff --git a/meson.build b/meson.build
index c44a77eb..e4875520 100644
--- a/meson.build
+++ b/meson.build
@@ -250,9 +271,4 @@ else
 
   # Ignore some Werror for clang until GLib fixes volatile usage in
   # GType registration.
-  if meson.get_compiler('c').get_id() == 'clang'
-    test_c_args += ['-Wno-incompatible-pointer-types-discards-qualifiers']
-  endif
+  #if meson.get_compiler('c').get_id() == 'clang'
+  #  test_c_args += ['-Wno-incompatible-pointer-types-discards-qualifiers']
+  #endif
 
   if buildtype != 'plain'
     test_c_args += '-fstack-protector-strong'
-- 
